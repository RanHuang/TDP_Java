# Setting up ccache
export USE_CCACHE=1
export CCACHE_DIR=~/src/.ccache
prebuilts/misc/linux-x86/ccache/ccache -M 50G
watch -n1 -d prebuilts/misc/linux-x86/ccache/ccache -s

source build/envsetup.sh
lunch aosp_hammerhead-userdebug
make fastboot adb
time make -j4

